{
  "name": "whiletruewar/koi_one_line",
  "description": "Library that can do a lot of things. Only takes 1 line of code :)",
  "license": "MIT",
  "keywords": [
    "NextPack",
    "Starter"
  ],
  "authors": [
    {
      "name": "WhileTrueWar",
      "email": "yeremia.blog@gmail.com",
      "homepage": "http://whiletruewar.com",
      "role": "r coaster"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/config": "5.2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "0.*",
    "symfony/var-dumper": "2.*"
  },
  "autoload": {
    "psr-4": {
      "whiletruewar\\koi_one_line\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "whiletruewar\\koi_one_line\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "stable"
}
